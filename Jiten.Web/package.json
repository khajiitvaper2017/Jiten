    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/seo": "^3.2.2",
    "@pinia/nuxt": "^0.10.1",
    "@primeuix/themes": "^1.2.5",
    "@primevue/forms": "^4.5.0",
    "@radix-icons/vue": "^1.0.0",
    "@tailwindcss/vite": "^4.1.17",
    "@unhead/vue": "^2.0.19",
    "chart.js": "^4.5.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
