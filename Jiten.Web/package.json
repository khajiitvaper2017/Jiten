    "primeicons": "^7.0.0",
    "primevue": "^4.5.0",
    "tailwindcss": "^4.1.17",
    "tailwindcss-primeui": "^0.5.1",
    "vue": "latest",
    "vue-chartjs": "^5.3.3",
